<TableTemplates>
	</TableTemplates>

<TableDescription>
	<Name>Tires</Name>
	<Type>TableDescription</Type>
	<Description>Definition for a single tire type.</Description>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Display_Name>Tire Name</Display_Name>
		<Description>Identifies the tire type.</Description>
		</Element>
	<Element>
		<Name>Tire_Foley</Name>
		<Type>Element</Type>
		<Display_Name>Tire Foley</Display_Name>
		<Description>The foley to associate with the tires.</Description>
		<Element>
			<Name>Foley_Name</Name>
			<Type>String</Type>
			<Display_Name>Tire Foley Name</Display_Name>
			<Description>The name of the foley.</Description>
			</Element>
		<Element>
			<Name>Blown_Foley_Name</Name>
			<Type>String</Type>
			<Display_Name>Blown Tire Foley Name</Display_Name>
			</Element>
		<Element>
			<Name>Materials</Name>
			<Type>List</Type>
			<Display_Name>Per-Material Foley Sets</Display_Name>
			<Description>A list of foley sets that are played when the tire is running on a specific material.</Description>
			<Element>
				<Name>Material_Foley_Set</Name>
				<Type>Element</Type>
				<Display_Name>Material Foley Set</Display_Name>
				<Description>A foley set associated with a specific material.</Description>
				<Required>false</Required>
				<Element>
					<Name>Material</Name>
					<Type>Selection</Type>
					<Description>The material to associate with this set.</Description>
					<Default>concrete</Default>
					<Choice>concrete</Choice>
					<Choice>wood</Choice>
					<Choice>glass</Choice>
					<Choice>grass</Choice>
					<Choice>gravel</Choice>
					<Choice>leaves,veg</Choice>
					<Choice>carpet</Choice>
					<Choice>metal</Choice>
					<Choice>thin metal</Choice>
					<Choice>fence metal</Choice>
					<Choice>water</Choice>
					<Choice>plastic</Choice>
					</Element>
				<Element>
					<Name>Foley_Sets</Name>
					<Type>List</Type>
					<Display_Name>Foley Sets</Display_Name>
					<Description>A list of foley sets (MPH/volume pairs) to interpolate across.</Description>
					<Element>
						<Name>Foley_Set</Name>
						<Type>Element</Type>
						<Display_Name>Foley Set</Display_Name>
						<Description>A single foley set (associates a volume with an MPH value)</Description>
						<Required>false</Required>
						<Element>
							<Name>MPH</Name>
							<Type>Int</Type>
							<Description>Miles per hour.</Description>
							<Default>0</Default>
							</Element>
						<Element>
							<Name>Volume</Name>
							<Type>Float</Type>
							<Display_Name>Volume Scale</Display_Name>
							<Description>The volume to play the foley at at the specified speed.</Description>
							<Default>0.0</Default>
							</Element>
						</Element>
					<Element_Name>Foley_Set.MPH</Element_Name>
					</Element>
				</Element>
			<Element_Name>Material_Foley_Set.Material</Element_Name>
			</Element>
		</Element>
	</TableDescription>

<EntryCategories>
	<Category>
		<Name>Templates</Name>
		</Category>
	</EntryCategories>
