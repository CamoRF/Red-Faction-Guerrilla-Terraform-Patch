		<Description>The list for mission briefings</Description>
		<Element>
			<Name>LineInfo</Name>
			<Type>Element</Type>
			<Description>Information for this line</Description>
			<Element>
				<Name>LineFrame</Name>
				<Type>Int</Type>
				<Description>The frame this line start on</Description>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>Persona</Name>
				<Type>Reference</Type>
				<Reference>
					<File>..\dlc\dlc01\tables\dlc01_rfg_voice_personas.xtbl</File>
