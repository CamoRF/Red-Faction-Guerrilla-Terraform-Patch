	<Element>
		<Name>Silence</Name>
		<Type>Element</Type>
		<Element>
			<Name>Ambience_Time</Name>
			<Type>Element</Type>
			<Display_Name>Transition To Ambience</Display_Name>
			<Description>Time is randomized between these values at the start of silence.</Description>
			<Element>
				<Name>Time_Min</Name>
				<Type>Int</Type>
				<Display_Name>Min Silence (seconds)</Display_Name>
				<Description>Minimum Silence time in seconds before transitioning to Neutral or Melodic Ambience.</Description>
				<Default>10</Default>
				</Element>
			<Element>
				<Name>Time_Max</Name>
				<Type>Int</Type>
				<Display_Name>Max Silence (seconds)</Display_Name>
				<Description>Maximum Silence time in seconds before transitioning to Neutral or Melodic Ambience.</Description>
				<Default>15</Default>
				</Element>
			</Element>
		<Element>
			<Name>Percentage_Neutral</Name>
			<Type>Int</Type>
			<Display_Name>Percentage to Neutral Ambience</Display_Name>
			<Default>0</Default>
			</Element>
		</Element>
	<Element>
		<Name>Ambience</Name>
		<Type>Element</Type>
		<Element>
			<Name>Silence_Time</Name>
			<Type>Element</Type>
			<Display_Name>Transition To Silence</Display_Name>
			<Description>Time is randomized between these values at the start of Neutral or Melodic Ambience, but not between them.</Description>
			<Element>
				<Name>Time_Min</Name>
				<Type>Int</Type>
				<Display_Name>Min Ambience (seconds)</Display_Name>
				<Description>Minimum Ambience play time before transitioning to silence.</Description>
				<Default>10</Default>
				</Element>
			<Element>
				<Name>Time_Max</Name>
				<Type>Int</Type>
				<Display_Name>Max Ambience (seconds)</Display_Name>
				<Description>Maximum Ambience play time before transitioning to silence.</Description>
				<Default>15</Default>
				</Element>
			</Element>
		<Element>
			<Name>Neutral</Name>
			<Type>Element</Type>
			<Element>
				<Name>Volume</Name>
				<Type>Float</Type>
				<Description>Neutral Ambience volume adjustment.</Description>
				<Default>.8</Default>
				</Element>
			<Element>
				<Name>Melodic_Time</Name>
				<Type>Element</Type>
				<Display_Name>Transition To Melodic</Display_Name>
				<Description>Time is randomized between these values at the start of Neutral, for transitioning to Melodic.</Description>
				<Element>
