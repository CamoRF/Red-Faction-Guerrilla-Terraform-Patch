				<Default>0</Default>
				<MinValue>0</MinValue>
				<MaxValue>5</MaxValue>
				</Element>
			<Element>
				<Name>liberated_marauder</Name>
				<Type>Float</Type>
				<Display_Name>Liberated Marauder Level</Display_Name>
