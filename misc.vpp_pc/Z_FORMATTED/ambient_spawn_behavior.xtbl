<TableDescription>
	<Name>Behaviors</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		</Element>
	<Element>
		<Name>Behavior_List</Name>
		<Type>Grid</Type>
		<Element>
			<Name>Behavior</Name>
			<Type>Element</Type>
			<Element>
				<Name>Animation_Override</Name>
				<Type>Reference</Type>
				<Display_Name>Animation Override</Display_Name>
				<Required>false</Required>
				<Reference>
					<File>anim_set_variants.xtbl</File>
					<Type>Override.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Ambient_Prop</Name>
				<Type>Reference</Type>
				<Display_Name>Ambient Prop</Display_Name>
				<Required>false</Required>
				<Reference>
					<File>ambient_prop_info.xtbl</File>
					<Type>amb_prop_info.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Chance</Name>
				<Type>Int</Type>
				<Description>Does not need to add up to 100%</Description>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>Day_Start</Name>
				<Type>Int</Type>
				<Default>0</Default>
				<MinValue>0</MinValue>
				<MaxValue>24</MaxValue>
				</Element>
			<Element>
				<Name>Day_End</Name>
				<Type>Int</Type>
				<Default>24</Default>
				<MinValue>0</MinValue>
				<MaxValue>24</MaxValue>
				</Element>
			<Element>
				<Name>MaleFemaleRatio</Name>
				<Type>Float</Type>
				<Description>Ratio of occurance on males to females, -1 means only males will spawn with this, 1 means only females, 0 means equally likely</Description>
				<Default>0.0</Default>
				</Element>
			<Element>
				<Name>Morale_Min</Name>
				<Type>Float</Type>
				<Default>0.0</Default>
				</Element>
			<Element>
				<Name>Morale_Max</Name>
				<Type>Float</Type>
				<Default>1.0</Default>
				</Element>
			</Element>
		<Num_Display_Rows>30</Num_Display_Rows>
		</Element>
	</TableDescription>

