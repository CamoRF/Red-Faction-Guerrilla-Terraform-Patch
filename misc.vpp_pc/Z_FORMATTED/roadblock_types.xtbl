<TableDescription>
	<Name>roadblock_data</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>barrier_pattern</Name>
		<Type>Reference</Type>
		<Display_Name>Barrier Pattern</Display_Name>
		<Description>Include this field if we should include barricades in this road block</Description>
		<Required>false</Required>
		<Default>Straight</Default>
		<Reference>
			<File>roadblock_formation.xtbl</File>
			<Type>formation_info.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>squad_grid</Name>
		<Type>Grid</Type>
		<Display_Name>Squad List</Display_Name>
		<Element>
			<Name>squad_data</Name>
			<Type>Element</Type>
			<Display_Name>Squad Data</Display_Name>
			<Element>
				<Name>squad_def</Name>
				<Type>Reference</Type>
				<Display_Name>Squad Definition</Display_Name>
				<Reference>
					<File>squads.xtbl</File>
					<Type>Squads.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>offset_x</Name>
				<Type>Float</Type>
				<Display_Name>Offset Position X</Display_Name>
				<Default>0.0</Default>
				<MinValue>-20</MinValue>
				<MaxValue>20</MaxValue>
				</Element>
			<Element>
				<Name>offset_z</Name>
				<Type>Float</Type>
				<Display_Name>Offset Position Z</Display_Name>
				<Default>0.0</Default>
				<MinValue>-20</MinValue>
				<MaxValue>20</MaxValue>
				</Element>
			<Element>
				<Name>vehicle_type</Name>
				<Type>Reference</Type>
				<Display_Name>Vehicle Type</Display_Name>
				<Required>false</Required>
				<Reference>
					<File>spawn_group_vehicle.xtbl</File>
					<Type>spawn_group_vehicle.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>stay_in_vehicle</Name>
				<Type>Selection</Type>
				<Display_Name>Stay In Vehicle</Display_Name>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>allow_pursuit</Name>
				<Type>Selection</Type>
				<Display_Name>Allow Pursuit</Display_Name>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>offset_rotation</Name>
				<Type>Float</Type>
				<Display_Name>Squad Rotation</Display_Name>
				<Description>Rotation of the squad spawn object, in degrees</Description>
				<Default>0.0</Default>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	</TableDescription>

