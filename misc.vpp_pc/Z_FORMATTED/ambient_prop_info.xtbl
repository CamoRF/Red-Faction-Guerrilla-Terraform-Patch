<TableDescription>
	<Name>amb_prop_info</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>Reference</Type>
		<Display_Name>Prop Name</Display_Name>
		<Reference>
			<File>items_3d.xtbl</File>
			<Type>Item.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>human_attach_point</Name>
		<Type>String</Type>
		<Display_Name>Human Attach Point Name</Display_Name>
		<Default>r-hand</Default>
		</Element>
	<Element>
		<Name>obj_attach_point</Name>
		<Type>String</Type>
		<Display_Name>Object Attach Point Name</Display_Name>
		<Default>weapon_handle</Default>
		</Element>
	<Element>
		<Name>walk_anim_name</Name>
		<Type>String</Type>
		<Display_Name>Walk Animation FileName</Display_Name>
		<Required>false</Required>
		</Element>
	<Element>
		<Name>stand_anim_name</Name>
		<Type>String</Type>
		<Display_Name>Stand Animation File Name</Display_Name>
		<Required>false</Required>
		</Element>
	<Element>
		<Name>exit_animation</Name>
		<Type>String</Type>
		<Display_Name>Exit Animation Tag</Display_Name>
		<Required>false</Required>
		</Element>
	<Element>
		<Name>exit_anim_trigger</Name>
		<Type>String</Type>
		<Display_Name>Exit Animation Trigger Name</Display_Name>
		<Required>false</Required>
		<Default>Props</Default>
		</Element>
	<Element>
		<Name>delete_on_exit</Name>
		<Type>Selection</Type>
		<Display_Name>Delete Prop On Exit</Display_Name>
		<Description>If this field is set to true, the prop will be deleted instead of dropped on the ground when the NPC detaches it</Description>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>always_on_effect</Name>
		<Type>Element</Type>
		<Display_Name>Always on Effect</Display_Name>
		<Description>Effect to create with this object</Description>
		<Required>false</Required>
		<Element>
			<Name>perm_effect_type</Name>
			<Type>Reference</Type>
			<Display_Name>Effect Type</Display_Name>
			<Reference>
				<File>effects.xtbl</File>
				<Type>Effect.Name</Type>
				<OpenSeparate>False</OpenSeparate>
				</Reference>
			</Element>
		<Element>
			<Name>perm_effect_attach_name</Name>
			<Type>String</Type>
			<Display_Name>Attach Point</Display_Name>
			</Element>
		</Element>
	<Element>
		<Name>anim_effect_list</Name>
		<Type>Grid</Type>
		<Display_Name>Animation Effect List</Display_Name>
		<Required>false</Required>
		<Element>
			<Name>anim_effect_info</Name>
			<Type>Element</Type>
			<Display_Name>Animation Effect</Display_Name>
			<Description>Effect to play during an animation</Description>
			<Element>
				<Name>anim_state_name</Name>
				<Type>String</Type>
				<Display_Name>Animation State</Display_Name>
				<Required>false</Required>
				</Element>
			<Element>
				<Name>anim_action_name</Name>
				<Type>String</Type>
				<Display_Name>Animation Action</Display_Name>
				<Required>false</Required>
				</Element>
			<Element>
				<Name>anim_effect_type</Name>
				<Type>Reference</Type>
				<Display_Name>Effect Type</Display_Name>
				<Description>Effect to play duting animation</Description>
				<Reference>
					<File>effects.xtbl</File>
					<Type>Effect.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>anim_effect_attach_name</Name>
				<Type>String</Type>
				<Display_Name>Attach Point Name</Display_Name>
				</Element>
			<Element>
				<Name>anim_start_tag</Name>
				<Type>String</Type>
				<Display_Name>Animation Start Tag</Display_Name>
				<Description>Animation Tag to start playing the effect. If none is provided, the effect starts at the beginning of the animation</Description>
				<Required>false</Required>
				</Element>
			<Element>
				<Name>anim_end_tag</Name>
				<Type>String</Type>
				<Display_Name>Animation End Tag</Display_Name>
				<Description>Animation End Tag. If none is provided, then the effect ends at the end of the end of the animation</Description>
				<Required>false</Required>
				</Element>
			<Element>
				<Name>looping_effect</Name>
				<Type>Selection</Type>
				<Display_Name>Looping Effect</Display_Name>
				<Default>True</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>attach_effect</Name>
				<Type>Selection</Type>
				<Display_Name>Attach Effect</Display_Name>
				<Description>True, if the effect should attach to the prop. False if it should play where the attach point was when the animation trigger was encountered.</Description>
				<Default>True</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Max_Children>5</Max_Children>
		<Min_Children>1</Min_Children>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	</TableDescription>

<EntryCategories>
	<Category>
		<Name>Entries</Name>
		</Category>
	</EntryCategories>
