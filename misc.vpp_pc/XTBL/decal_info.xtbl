<root>
<stream2_container_description>
	<container>decal_info</container>
	<container_name>name</container_name>

	<container_preload_category>true</container_preload_category>

	<!-- This is used to filter out at different stages. The name is the xml element needed
		to make the decision. -->
	<container_filter>preload</container_filter>

	<container_type>GSC_DECALS</container_type>
	<container_preload_type>GSC_DECALS_PRELOAD</container_preload_type>
	<container_contents>material
		<name>filename</name>
		<ext>mat</ext>
		<bitmap_file_preload_ext>slw3</bitmap_file_preload_ext>
		<bitmap_file_ext>cpeg</bitmap_file_ext>
		<type>GSP_MATERIAL</type>
		<report_errors>false</report_errors>
	</container_contents>
</stream2_container_description>

