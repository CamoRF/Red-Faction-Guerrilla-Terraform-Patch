<TableDescription>
	<Name>Loading_screen_tips</Name>
	<Type>TableDescription</Type>
	<Display_Name>Loading screen tips</Display_Name>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>Name of the loading screen tip.  Also functions as the header for the tip.</Description>
		<MaxLength>32</MaxLength>
		</Element>
	<Element>
		<Name>Text</Name>
		<Type>String</Type>
		<Description>This is the body of the text for the loading screen tip.</Description>
		<MaxLength>296</MaxLength>
		<MultiLine>true</MultiLine>
		</Element>
	<Element>
		<Name>Icon</Name>
		<Type>String</Type>
		<Description>This is the name of the icon for this loading screen tip.  If you don't know the name of the icon, Matt Gawalek and Jason Montgomery can help you!</Description>
		<MaxLength>32</MaxLength>
		</Element>
	<Element>
		<Name>Type</Name>
		<Type>Selection</Type>
		<Description>This specifies the game type that this tip is for (single player, multi player, etc.)</Description>
		<Default>Multiplayer</Default>
		<Choice>Single Player</Choice>
		<Choice>Multiplayer</Choice>
		<Choice>Both</Choice>
		</Element>
	</TableDescription>

<EntryCategories>
	<Category>
		<Name>Entries</Name>
		</Category>
	</EntryCategories>
