<TableTemplates>
	</TableTemplates>

<TableDescription>
	<Name>Air_Bomb</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>The unique name of the bombing senario.</Description>
		<Unique>true</Unique>
		</Element>
	<Element>
		<Name>Projectile</Name>
		<Type>Reference</Type>
		<Description>The weapon/projectile type that is being dropped.</Description>
		<Reference>
			<File>weapons.xtbl</File>
			<Type>Weapon.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Num_Projectiles</Name>
		<Type>Int</Type>
		<Display_Name>Num Projectiles</Display_Name>
		<Description>The number of projectiles that should be created for each incoming bomb.</Description>
		<Default>1</Default>
		<MinValue>1</MinValue>
		<MaxValue>5</MaxValue>
		<UseSlider>True</UseSlider>
		</Element>
	<Element>
		<Name>Min_Spread</Name>
		<Type>Float</Type>
		<Display_Name>Min Spread</Display_Name>
		<Description>The minimum spread for multiple projectile bombs (in degrees).</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Max_Spread</Name>
		<Type>Float</Type>
		<Display_Name>Max Spread</Display_Name>
		<Description>The maximum spread for multiple projectile bombs (in degrees).</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Min_Proj_Delay</Name>
		<Type>Float</Type>
		<Display_Name>Min Time Between Projectiles</Display_Name>
		<Description>The minimum amount of time between projectiles from the same bomb (in seconds).</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Max_Proj_Delay</Name>
		<Type>Float</Type>
		<Display_Name>Max Time Between Projectiles</Display_Name>
		<Description>The maximum amount of time between projectiles from the same bomb (in seconds).</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Height</Name>
		<Type>Float</Type>
		<Description>The default height to drop the bombs from (in meters).</Description>
		<Default>100</Default>
		</Element>
	<Element>
		<Name>Radius</Name>
		<Type>Float</Type>
		<Description>The default radius of the bombing (in meters)</Description>
		<Default>50</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Vehicle_radius</Name>
		<Type>Float</Type>
		<Display_Name>Vehicle Radius</Display_Name>
		<Description>Radius of the bombing when in a vehicle</Description>
		<Default>50</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Rate_min</Name>
		<Type>Float</Type>
		<Display_Name>Rate Min</Display_Name>
		<Description>Min amount of seconds between when projectiles are dropped.</Description>
		<Default>1</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Rate_max</Name>
		<Type>Float</Type>
		<Display_Name>Rate Max</Display_Name>
		<Description>Max amount of seconds between when projectiles are dropped.</Description>
		<Default>1</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Angle_min</Name>
		<Type>Float</Type>
		<Display_Name>Angle Min</Display_Name>
		<Description>Minimum angle in degrees of incoming projectiles.  90 is verticle, 0 is horizontal (and not allowed).</Description>
		<Default>90</Default>
		<MinValue>1</MinValue>
		<MaxValue>90</MaxValue>
		</Element>
	<Element>
		<Name>Angle_max</Name>
		<Type>Float</Type>
		<Display_Name>Angle Max</Display_Name>
		<Description>Maximum angle in degrees of incoming projectiles.  90 is verticle, 0 is horizontal (and not allowed).</Description>
		<Default>90</Default>
		<MinValue>1</MinValue>
		<MaxValue>90</MaxValue>
		</Element>
	<Element>
		<Name>Min_player_look_dot_prod</Name>
		<Type>Float</Type>
		<Display_Name>Min Player Look Dot Prod</Display_Name>
		<Description>-1 indicates that all projectiles are allowed, 1 indicates that only projectiles directly in front of the player (which is practically none) will be allowed.  If an attack object is in range, it will not take this measurement into account.  Note: setting this too high may result in projectiles not being fired in a timely fashion.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		<MinValue>-1</MinValue>
		<MaxValue>1</MaxValue>
		</Element>
	</TableDescription>

<EntryCategories>
	<Category>
		<Name>Entries</Name>
		</Category>
	</EntryCategories>
</root>