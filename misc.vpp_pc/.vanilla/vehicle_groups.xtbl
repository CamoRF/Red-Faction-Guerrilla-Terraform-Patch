<TableDescription>
	<Name>Vehicle_Group</Name>
	<Type>TableDescription</Type>
	<Display_Name>Vehicle Group</Display_Name>
	<MaxCount>16</MaxCount>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Display_Name>Group Name</Display_Name>
		<Description>Unique name of the vehicle used by the game.</Description>
		<Unique>true</Unique>
		<Default>NoName</Default>
		<MaxLength>23</MaxLength>
		</Element>
	<Element>
		<Name>Max_Hitpoints</Name>
		<Type>Int</Type>
		<Display_Name>Hitpoints</Display_Name>
		<Description>Maximum hitpoints for the vehicle.</Description>
		<Required>false</Required>
		<Default>5000</Default>
		<MinValue>0</MinValue>
		<MaxValue>1000000</MaxValue>
		</Element>
	<Element>
		<Name>Mass</Name>
		<Type>Float</Type>
		<Description>Mass of the vehicle in kilograms.</Description>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Center_Of_Mass_Y</Name>
		<Type>Float</Type>
		<Display_Name>Center of Mass Y</Display_Name>
		<Description>Offset of the center of mass from the position in the y direction.</Description>
		<Required>false</Required>
		<Default>0.07</Default>
		</Element>
	<Element>
		<Name>Center_Of_Mass_Z</Name>
		<Type>Float</Type>
		<Display_Name>Center of Mass Z</Display_Name>
		<Description>Offset of the center of mass from the position in the y direction.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Cameras</Name>
		<Type>List</Type>
		<Required>false</Required>
		<Element>
			<Name>Camera</Name>
			<Type>Element</Type>
			<Element>
				<Name>Lookat_Offset</Name>
				<Type>Vector</Type>
				<Display_Name>Lookat Offset</Display_Name>
				<Description>The position (offset from the center of the car) that the camera should focus on when tracking the vehicle.</Description>
				<Required>false</Required>
				<X>
					<Name>X</Name>
					<Type>Float</Type>
					<Default>0.0</Default>
					</X>
				<Y>
					<Name>Y</Name>
					<Type>Float</Type>
					<Default>1.0</Default>
					</Y>
				<Z>
					<Name>Z</Name>
					<Type>Float</Type>
					<Default>0.0</Default>
					</Z>
				</Element>
			<Element>
				<Name>Lookat_Vehicle_Offset</Name>
				<Type>Vector</Type>
				<Display_Name>Lookat Vehicle Offset</Display_Name>
				<Description>Camera lookat offset in vehicle space.</Description>
				<Required>false</Required>
				<X>
					<Name>X</Name>
					<Type>Float</Type>
					<Default>0.0</Default>
					</X>
				<Y>
					<Name>Y</Name>
					<Type>Float</Type>
					<Default>0.0</Default>
					</Y>
				<Z>
					<Name>Z</Name>
					<Type>Float</Type>
					<Default>0.0</Default>
					</Z>
				</Element>
			<Element>
				<Name>Follow_Distance</Name>
				<Type>Float</Type>
				<Display_Name>Follow Distance</Display_Name>
				<Description>Distance from the car that the camera should follow.</Description>
				<Required>false</Required>
				<Default>10.0</Default>
				</Element>
			<Element>
				<Name>Follow_Height</Name>
				<Type>Float</Type>
				<Display_Name>Follow Height</Display_Name>
				<Description>The height off the ground that the camera should snap back to.</Description>
				<Required>false</Required>
				<Default>0.75</Default>
				</Element>
			<Element>
				<Name>Y_Axis_Rotation</Name>
				<Type>Float</Type>
				<Display_Name>Y-axis Rotation</Display_Name>
				<Description>Speed multiplier of camera rotation around the Y axis.</Description>
				<Required>false</Required>
				<Default>1.0</Default>
				</Element>
			<Element>
				<Name>Camera_Steer_Angle</Name>
				<Type>Float</Type>
				<Display_Name>Camera Steer Angle</Display_Name>
				<Description>Max camera steer angle in radians.</Description>
				<Required>false</Required>
				<Default>0.0</Default>
				</Element>
			<Element>
				<Name>Camera_Steer_Speed</Name>
				<Type>Float</Type>
				<Display_Name>Camera Steer Speed</Display_Name>
				<Description>Camera steer angle chase speed in radians/sec</Description>
				<Required>false</Required>
				<Default>0.0</Default>
				</Element>
			<Element>
				<Name>Turret_Camera</Name>
				<Type>Selection</Type>
				<Display_Name>Turret Camera</Display_Name>
				<Description>Should it enforce that this camera be used for the turret?</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Max_Children>3</Max_Children>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Energy_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Energy Scale</Display_Name>
		<Description>Energy scale used for collisions, higher values will cause the vehicle to break through stuff easier.</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>Collision_Mass_Scalar</Name>
		<Type>Float</Type>
		<Display_Name>Collision Mass Scalar (Player)</Display_Name>
		<Required>false</Required>
		<Default>1.0</Default>
		<MinValue>1</MinValue>
		<MaxValue>10</MaxValue>
		</Element>
	<Element>
		<Name>Collision_Damage_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Collision Damage Scale</Display_Name>
		<Description>This scale gets applied to all vehicle collision damage.</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>Terrain_Damage_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Terrain Damage Scale</Display_Name>
		<Description>This scales the amount of damage applied by colliding with terrain</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>Vehicle_Damage_Scale</Name>
		<Type>Float</Type>
		<Display_Name>Vehicle Damage Scale</Display_Name>
		<Description>A scalar applied to vehicle-vehicle collision damage, on top of collision damage scale.</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>Force_Break_Links</Name>
		<Type>Selection</Type>
		<Display_Name>Force Break Links</Display_Name>
		<Description>This vehicle should force links to break when it hits a destroyable regardless of how fast it is moving</Description>
		<Required>false</Required>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Static_Load_Friction</Name>
		<Type>Float</Type>
		<Display_Name>Static Load Friction</Display_Name>
		<Description>Averages the static and dynamic load contributions for friction.  A value of 0.0 means it's all dynamic.</Description>
		<Required>false</Required>
		<Default>0</Default>
		</Element>
	<Element>
		<Name>AI_Static_Load_Friction</Name>
		<Type>Float</Type>
		<Display_Name>AI Static Load Friction</Display_Name>
		<Description>Averages the static and dynamic load contributions for friction.  A value of 0.0 means it's all dynamic. (For AI vehicles)</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Viscosity_Friction</Name>
		<Type>Float</Type>
		<Display_Name>Viscosity Friction</Display_Name>
		<Description>An extra velocity dependent friction factor. This factor increases the friction if the car slides. Values between 0.0 and 0.1 are reasonable.</Description>
		<Required>false</Required>
		<Default>0.05</Default>
		<MinValue>0</MinValue>
		<MaxValue>0.1</MaxValue>
		</Element>
	<Element>
		<Name>Roll_Torque_Factor</Name>
		<Type>Float</Type>
		<Display_Name>Roll Torque Factor</Display_Name>
		<Description>Scales the torques applied by the simulation at the roll axis by the given factor.</Description>
		<Required>false</Required>
		<Default>0.25</Default>
		</Element>
	<Element>
		<Name>Pitch_Torque_Factor</Name>
		<Type>Float</Type>
		<Display_Name>Pitch Torque Factor</Display_Name>
		<Description>Scales the torques applied by the simulation at the pitch axis by the given factor.</Description>
		<Required>false</Required>
		<Default>0.5</Default>
		</Element>
	<Element>
		<Name>Yaw_Torque_Factor</Name>
		<Type>Float</Type>
		<Display_Name>Yaw Torque Factor</Display_Name>
		<Description>Scales the torques applied by the simulation at the yaw axis by the given factor.</Description>
		<Required>false</Required>
		<Default>0.35</Default>
		</Element>
	<Element>
		<Name>Yaw_Torque_Scalar</Name>
		<Type>Float</Type>
		<Display_Name>Yaw Torque Scalar (Player)</Display_Name>
		<Required>false</Required>
		<Default>1.0</Default>
		<MinValue>0</MinValue>
		<MaxValue>10</MaxValue>
		</Element>
	<Element>
		<Name>Extra_Steering_Torque</Name>
		<Type>Float</Type>
		<Display_Name>Extra Steering Torque</Display_Name>
		<Description>An extra torque which is applied to the car when steering is pointing left or right.</Description>
		<Required>false</Required>
		<Default>-0.5</Default>
		</Element>
	<Element>
		<Name>Roll_Unit_Inertia</Name>
		<Type>Float</Type>
		<Display_Name>Roll Unit Inertia</Display_Name>
		<Description>The rotation inertia around the roll axis for a car of the mass 1.0f kilogram, 1.0 is normal.</Description>
		<Required>false</Required>
		<Default>0.4</Default>
		</Element>
	<Element>
		<Name>Pitch_Unit_Inertia</Name>
		<Type>Float</Type>
		<Display_Name>Pitch Unit Inertia</Display_Name>
		<Description>The rotation inertia around the pitch axis for a car of the mass 1.0f kilogram, 1.0 is normal.</Description>
		<Required>false</Required>
		<Default>1</Default>
		</Element>
	<Element>
		<Name>Yaw_Unit_Inertia</Name>
		<Type>Float</Type>
		<Display_Name>Yaw Unit Inertia</Display_Name>
		<Description>The rotation inertia around the yaw axis for a car of the mass 1.0f kilogram, 1.0 is normal.</Description>
		<Required>false</Required>
		<Default>1</Default>
		</Element>
	<Element>
		<Name>AI_Yaw_Unit_Inertia</Name>
		<Type>Float</Type>
		<Display_Name>AI Yaw Unit Inertia</Display_Name>
		<Description>The rotation inertia around the yaw axis for a car of the mass 1.0f kilogram, 1.0 is normal. (For AI vehicles)</Description>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>AI_Max_Braking_Decel</Name>
		<Type>Float</Type>
		<Display_Name>AI Maximum Braking Decel.</Display_Name>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>AI_Max_Radial_Accel</Name>
		<Type>Float</Type>
		<Display_Name>AI Maximum Radial Accel.</Display_Name>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Alert_Level_Multiplier</Name>
		<Type>Float</Type>
		<Display_Name>Alert Level Multiplier</Display_Name>
		<Required>false</Required>
		<Default>1.0</Default>
		</Element>
	<Element>
		<Name>LODs</Name>
		<Type>List</Type>
		<Display_Name>Levels of Detail</Display_Name>
		<Description>A list of available levels-of-detail for the vehicle.</Description>
		<Required>false</Required>
		<Element>
			<Name>LOD_Distance</Name>
			<Type>Float</Type>
			<Display_Name>LOD Distance</Display_Name>
			<Description>The maximum distance for the LOD in meters.</Description>
			<Default>10</Default>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		<Element_Name>LOD_Distance</Element_Name>
		</Element>
	<Element>
		<Name>Autobrake_Speed</Name>
		<Type>Float</Type>
		<Display_Name>Autobrake Speed</Display_Name>
		<Description>Speend (in m/s) under which vehicle automatically brakes.</Description>
		<Required>false</Required>
		<Default>3.0</Default>
		<MinValue>0</MinValue>
		<MaxValue>1000</MaxValue>
		</Element>
	<Element>
		<Name>EnforcedMaxSpeed</Name>
		<Type>Float</Type>
		<Display_Name>Enforced Max Speed</Display_Name>
		<Description>Max speed enforced outside of Havok.  The Havok engine model (or other in-game limits) may limit top speed even more.</Description>
		<Required>false</Required>
		<Default>90</Default>
		<MinValue>5</MinValue>
		<MaxValue>120</MaxValue>
		</Element>
	<Element>
		<Name>EnforcedMaxRSpeed</Name>
		<Type>Float</Type>
		<Display_Name>Enforced Max Rev. Speed</Display_Name>
		<Description>Enforced Max Speed for reverse.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		<MinValue>5</MinValue>
		<MaxValue>90</MaxValue>
		</Element>
	<Element>
		<Name>Engine</Name>
		<Type>Element</Type>
		<Display_Name>Engine Parameters</Display_Name>
		<Description>Parameters defining the engine of the vehicle.</Description>
		<Element>
			<Name>Torque</Name>
			<Type>Float</Type>
			<Description>Maximum gross torque the engine can produce in Newton-meters.</Description>
			<Default>0.0</Default>
			</Element>
		<Element>
			<Name>Min_RPM</Name>
			<Type>Float</Type>
			<Display_Name>Minimum RPM</Display_Name>
			<Description>Minimum RPM of the engine.</Description>
			<Required>false</Required>
			<Default>1000</Default>
			</Element>
		<Element>
			<Name>Opt_RPM</Name>
			<Type>Float</Type>
			<Display_Name>Optimal RPM</Display_Name>
			<Description>Optimal RPM of the engine (where it makes the most horsepower).</Description>
			<Required>false</Required>
			<Default>5500</Default>
			</Element>
		<Element>
			<Name>Max_RPM</Name>
			<Type>Float</Type>
			<Display_Name>Maximum RPM</Display_Name>
			<Description>Maximum RPM of the engine.</Description>
			<Required>false</Required>
			<Default>7500</Default>
			</Element>
		<Element>
			<Name>Min_RPM_Torque_Factor</Name>
			<Type>Float</Type>
			<Display_Name>Minimum RPM Torque Factor</Display_Name>
			<Description>Fraction of the engine torque (0.0 - 1.0) at minimum RPM.</Description>
			<Required>false</Required>
			<Default>0.8</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Max_RPM_Torque_Factor</Name>
			<Type>Float</Type>
			<Display_Name>Maximum RPM Torque Factor</Display_Name>
			<Description>Minimum RPM of the engine.</Description>
			<Required>false</Required>
			<Default>0.8</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Min_RPM_Resistance</Name>
			<Type>Float</Type>
			<Display_Name>Minimum RPM Resistance</Display_Name>
			<Description>Fraction of the engine torque (0.0 - 1.0) at minimum RPM due to resistance.</Description>
			<Required>false</Required>
			<Default>0.05</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Opt_RPM_Resistance</Name>
			<Type>Float</Type>
			<Display_Name>Optimal RPM Resistance</Display_Name>
			<Description>Fraction of the engine torque (0.0 - 1.0) at optimal RPM due to resistance.</Description>
			<Required>false</Required>
			<Default>0.1</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Max_RPM_Resistance</Name>
			<Type>Float</Type>
			<Display_Name>Maximum RPM Resistance</Display_Name>
			<Description>Fraction of the engine torque (0.0 - 1.0) at maximum RPM due to resistance.</Description>
			<Required>false</Required>
			<Default>0.3</Default>
			<MinValue>0</MinValue>
			<MaxValue>1</MaxValue>
			</Element>
		<Element>
			<Name>Reverse_torque_multiplier</Name>
			<Type>Float</Type>
			<Display_Name>Reverse Torque Multiplier</Display_Name>
			<Required>false</Required>
			<Default>1.0</Default>
			</Element>
		<Element>
			<Name>Force_assist</Name>
			<Type>Float</Type>
			<Display_Name>Force Assist</Display_Name>
			<Required>false</Required>
			<Default>0.0</Default>
			<MinValue>0</MinValue>
			</Element>
		</Element>
	<Element>
		<Name>Axles</Name>
		<Type>List</Type>
		<Display_Name>Axle Parameters</Display_Name>
		<Description>Parameters defining the axles of the vehicle.</Description>
		<Element>
			<Name>Axle</Name>
			<Type>Element</Type>
			<Description>A single axle of the vehicle.</Description>
			<Element>
				<Name>Name</Name>
				<Type>String</Type>
				<Display_Name>Axle Name</Display_Name>
				<Description>The name for the axle.</Description>
				<Default>Axle</Default>
				</Element>
			<Element>
				<Name>Wheel_Does_Steer</Name>
				<Type>Selection</Type>
				<Display_Name>Wheel Does Steer</Display_Name>
				<Description>Whether or not the wheel is part of steering.</Description>
				<Default>true</Default>
				<Choice>true</Choice>
				<Choice>false</Choice>
				</Element>
			<Element>
				<Name>Wheel_Does_Handbrake</Name>
				<Type>Selection</Type>
				<Display_Name>Wheel Does Handbrake</Display_Name>
				<Description>Whether or not the wheel is part of the handbrake.</Description>
				<Default>true</Default>
				<Choice>true</Choice>
				<Choice>false</Choice>
				</Element>
			<Element>
				<Name>Wheel_Engine_Torque</Name>
				<Type>Float</Type>
				<Display_Name>Wheel Engine Torque</Display_Name>
				<Description>Amount of torque the wheel gets from the engine (0.0 - 1.0).  
Note: This will be scaled in game so that the total of all the wheels is 1.0.</Description>
