<TableDescription>
	<Name>MissionBriefingLines</Name>
	<Type>TableDescription</Type>
	<Display_Name>Mission Briefing Lines</Display_Name>
	<Element>
		<Name>Name</Name>
		<Type>Reference</Type>
		<Display_Name>Mission Briefing Name</Display_Name>
		<Reference>
			<File>missions.xtbl</File>
			<Type>mission.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>MissionDescription</Name>
		<Type>Reference</Type>
		<Display_Name>Mission Description</Display_Name>
		<Reference>
			<File>missions.xtbl</File>
			<Type>mission.description</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>MissionBriefingList</Name>
		<Type>List</Type>
		<Display_Name>Mission Briefing List of Lines</Display_Name>
		<Description>The list for mission briefings</Description>
		<Element>
			<Name>LineInfo</Name>
			<Type>Element</Type>
			<Description>Information for this line</Description>
			<Element>
				<Name>LineFrame</Name>
				<Type>Int</Type>
				<Description>The frame this line start on</Description>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>Persona</Name>
				<Type>Reference</Type>
				<Reference>
					<File>rfg_voice_personas.xtbl</File>
					<Type>Persona.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Line</Name>
				<Type>Reference</Type>
				<Description>The text for this line</Description>
				<Reference>
					<File>rfg_lines_scripted.xtbl</File>
					<Type>voice_line_data.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>LineColor</Name>
				<Type>Color</Type>
				<Description>Line Color of the item</Description>
				<R>
					<Name>R</Name>
					<Type>Int</Type>
					<Default>255</Default>
					</R>
				<G>
					<Name>G</Name>
					<Type>Int</Type>
					<Default>255</Default>
					</G>
				<B>
					<Name>B</Name>
					<Type>Int</Type>
					<Default>255</Default>
					</B>
				</Element>
			<Element>
				<Name>PauseUntilPrevAudioDone</Name>
				<Type>Selection</Type>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		<Element_Name>LineInfo.LineFrame</Element_Name>
		</Element>
	<Element>
		<Name>BackgroundMusic</Name>
		<Type>Reference</Type>
		<Display_Name>Background Music Name</Display_Name>
		<Reference>
			<File>foley_music.xtbl</File>
			<Type>Foley.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>BackgroundMusicVolume</Name>
		<Type>Float</Type>
		<Display_Name>Background Music Volume</Display_Name>
		<Default>0.6</Default>
		<MinValue>0</MinValue>
		<MaxValue>1</MaxValue>
		<UseSlider>True</UseSlider>
		</Element>
	<Element>
		<Name>BinkVideoVolume</Name>
		<Type>Float</Type>
		<Display_Name>Bink Video Volume</Display_Name>
		<Default>0.8</Default>
		<MinValue>0</MinValue>
		<MaxValue>5</MaxValue>
		<UseSlider>True</UseSlider>
		</Element>
	<Element>
		<Name>loop_frames</Name>
		<Type>Int</Type>
		<Display_Name>Num Frames to Loop</Display_Name>
		<Description>The number of video frames that should loop at the once the video reaches the end.</Description>
		<Default>15</Default>
		</Element>
	</TableDescription>

<EntryCategories>
