<TableDescription>
	<Name>Handbook_Entry</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>Name of this entry. This is NOT used code-side, except for BTFP where it is used for cross-reference to the BTFP node pair number</Description>
		</Element>
	<Element>
		<Name>Title</Name>
		<Type>String</Type>
		<Description>Title of this entry.</Description>
		</Element>
	<Element>
		<Name>Description</Name>
		<Type>String</Type>
		<Description>Description for this entry</Description>
		<Required>false</Required>
		</Element>
	<Element>
		<Name>Info</Name>
		<Type>String</Type>
		<Description>This is used for entries on certain pages that require an additional line of text.  The contents of this string depends on the page the entry is for.</Description>
		<Required>false</Required>
		</Element>
	<Element>
		<Name>Image</Name>
		<Type>Filename</Type>
		<Description>Image to show with this entry.  (Either a video or image can be specified for a single entry, not both)</Description>
		<Required>false</Required>
		<Extension>tga</Extension>
		<StartingPath>data</StartingPath>
		<ShowPreload>False</ShowPreload>
		</Element>
	<Element>
		<Name>Video</Name>
		<Type>Filename</Type>
		<Description>Video for this entry. (Either a video or image can be specified for a single entry, not both)</Description>
		<Required>false</Required>
		<MaxLength>64</MaxLength>
		<Extension>bik</Extension>
		<StartingPath>data</StartingPath>
		<ShowPreload>True</ShowPreload>
		</Element>
	<Element>
		<Name>Voice_Line</Name>
		<Type>Reference</Type>
		<Display_Name>Voice Line</Display_Name>
		<Description>Optional Voice line</Description>
		<Required>false</Required>
		<Reference>
			<File>rfg_lines_scripted.xtbl</File>
			<Type>voice_line_data.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Unlocked</Name>
		<Type>Selection</Type>
		<Description>True if this entry should always be unlocked, false if it must be unlocked in-game.</Description>
		<Default>True</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	</TableDescription>

