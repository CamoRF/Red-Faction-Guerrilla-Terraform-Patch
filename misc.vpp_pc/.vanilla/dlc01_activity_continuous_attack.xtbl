		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>squad_list</Name>
		<Type>List</Type>
		<Display_Name>Squad List</Display_Name>
		<Element>
			<Name>squad_info</Name>
			<Type>Element</Type>
			<Display_Name>Squad Info</Display_Name>
			<Element>
				<Name>Sub_Group_Index</Name>
				<Type>Int</Type>
				<Display_Name>Sub-group Index</Display_Name>
				<Description>Specifies a sub-group for squad definitions.  Only one sub-group can be active at any time, and only squads which are members of that subgroup will be elligible for spawning.</Description>
				<Required>false</Required>
				<Default>0</Default>
				</Element>
			<Element>
				<Name>squad_def</Name>
				<Type>Reference</Type>
				<Display_Name>Squad Definition</Display_Name>
				<Reference>
					<File>..\dlc\dlc01\tables\dlc01_squads.xtbl</File>
					<Type>Squads.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>vehicle_group</Name>
				<Type>Reference</Type>
				<Display_Name>Vehicle Group</Display_Name>
				<Required>false</Required>
				<Reference>
					<File>..\dlc\dlc01\tables\dlc01_spawn_group_vehicle.xtbl</File>
					<Type>spawn_group_vehicle.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>stay_in_vehicle</Name>
				<Type>Selection</Type>
				<Display_Name>Stay In Vehicle</Display_Name>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>veh_hp_scale_factor</Name>
				<Type>Float</Type>
				<Display_Name>Vehicle HP Scale Factor</Display_Name>
				<Description>Vehicle HP will be multiplied by this factor at spawn time.</Description>
