<TableDescription>
	<Name>continuous_attack_data</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		</Element>
	<Element>
		<Name>attacker_count</Name>
		<Type>Int</Type>
		<Display_Name>Default Attacker Count</Display_Name>
		<Default>0</Default>
		</Element>
	<Element>
		<Name>respawn_period</Name>
		<Type>Float</Type>
		<Display_Name>Respawn Period (sec)</Display_Name>
		<Default>0.0</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Type</Name>
		<Type>Selection</Type>
		<Description>Set this to true if you want the squads to follow a patrol.  Otherwise set this to false for the squad to find their own path to the target.</Description>
		<Default>Default</Default>
		<Choice>Default</Choice>
		<Choice>Patrol</Choice>
		</Element>
	<Element>
		<Name>Show_Squad_Icons</Name>
