<TableDescription>
	<Name>Activity_riding_shotgun</Name>
	<Type>TableDescription</Type>
	<Display_Name>Activity: Riding Shotgun</Display_Name>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		</Element>
	<Element>
		<Name>Caption</Name>
		<Type>Reference</Type>
		<Description>The caption of this activity to be displayed on the HUD</Description>
		<Required>false</Required>
		<Reference>
			<File>localized_exports\Activity_riding_shotgun_text.xtbl</File>
			<Type>Riding_Shotgun_Identifier.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Base_Data</Name>
		<Type>Reference</Type>
		<Display_Name>Layers / Scripts</Display_Name>
		<Description>Select the base activity managed layers/scripts for this riding shotgun</Description>
		<Required>false</Required>
		<Reference>
			<File>activity.xtbl</File>
			<Type>base_activity.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Activity_Start_Radio_Message</Name>
		<Type>Reference</Type>
		<Display_Name>Activity Start Radio Message</Display_Name>
		<Description>The radio message that announces the activity.</Description>
		<Required>false</Required>
		<Reference>
			<File>rfg_lines_scripted.xtbl</File>
			<Type>voice_line_data.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Vrail_data</Name>
		<Type>Reference</Type>
		<Display_Name>Vrail Data</Display_Name>
		<Reference>
			<File>vehicle_rail_data.xtbl</File>
			<Type>VRailData.Name</Type>
			<OpenSeparate>False</OpenSeparate>
			</Reference>
		</Element>
	<Element>
		<Name>Target_Score</Name>
		<Type>Float</Type>
		<Display_Name>Target Score</Display_Name>
		<Description>The amount of damage required to complete the activity in millions of credits.</Description>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Par_Score</Name>
		<Type>Float</Type>
		<Description>The score required to receive the extra reward.</Description>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Max_Civ_Penalty</Name>
		<Type>Float</Type>
		<Display_Name>Max Civilian Penalty</Display_Name>
		<Description>The largest percentage deducted from the player's score for civilian deaths. (XX.X %)</Description>
		<Default>50.0</Default>
		<MinValue>0</MinValue>
		<MaxValue>100</MaxValue>
		</Element>
	<Element>
		<Name>Max_Civilians</Name>
		<Type>Int</Type>
		<Display_Name>Max Num Civilians</Display_Name>
		<Description>The number of civilian deaths required to incure the maximum civilian penalty.  (if set to 0, no penalty will be applied for civilian deaths)</Description>
		<Default>10</Default>
		<MinValue>0</MinValue>
		</Element>
	<Element>
		<Name>Vehicle_hit_points</Name>
		<Type>Int</Type>
		<Display_Name>Vehicle Hit Points</Display_Name>
		<Description>Override the hit points of the vrail vehicle.</Description>
		<Required>false</Required>
		<Default>0</Default>
		<MinValue>1</MinValue>
		</Element>
	<Element>
		<Name>Alert_level_start</Name>
		<Type>Int</Type>
		<Display_Name>Alert Level Start</Display_Name>
		<Required>false</Required>
		<Default>0</Default>
		<MinValue>0</MinValue>
		<MaxValue>3</MaxValue>
		</Element>
	<Element>
		<Name>Extra_reward</Name>
		<Type>Float</Type>
		<Display_Name>Extra Morale Reward</Display_Name>
		<Description>Amount of reward per percentage point over the final goal you get on top of the regular reward.</Description>
		<Required>false</Required>
		<Default>0.0</Default>
		</Element>
	<Element>
		<Name>Standard_Rewards</Name>
		<Type>Element</Type>
		<Display_Name>Standard Rewards</Display_Name>
		<Element>
			<Name>EDF_Control_Reduction</Name>
			<Type>Float</Type>
			<Display_Name>EDF Control Reduction</Display_Name>
			<Required>false</Required>
			<Default>0.0</Default>
			</Element>
		<Element>
			<Name>Morale_Increase</Name>
			<Type>Float</Type>
			<Display_Name>Morale Increase</Display_Name>
			<Required>false</Required>
			<Default>0.0</Default>
			</Element>
		<Element>
			<Name>Salvage_Reward</Name>
			<Type>Int</Type>
			<Display_Name>Salvage Reward</Display_Name>
			<Required>false</Required>
			<Default>0</Default>
			</Element>
		</Element>
	</TableDescription>

<EntryCategories>
