<TableDescription>
	<Name>Tool_Tips</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		<Description>Name of this tool tip, used for reference only.</Description>
		<MaxLength>32</MaxLength>
		</Element>
	<Element>
		<Name>Title</Name>
		<Type>String</Type>
		<Description>The title of the tool tip as it should appear in game.</Description>
		</Element>
	<Element>
		<Name>Description</Name>
		<Type>String</Type>
		<Description>The description of the tool tip as it should appear in game.</Description>
		<MaxLength>256</MaxLength>
		</Element>
	<Element>
		<Name>Icon</Name>
		<Type>Selection</Type>
		<Description>The image name of the icon to use for the tool tip (should be set by MattG or JasonM until a reference table is created)</Description>
		<Required>false</Required>
		<Default>ui_hud_tip_icon</Default>
		<Choice>ui_hud_tip_icon</Choice>
		<Choice>ui_map_icon_safe_house</Choice>
		<Choice>ui_map_icon_mission_start</Choice>
		<Choice>ui_map_icon_objective</Choice>
		<Choice>ui_map_icon_pmedium</Choice>
		<Choice>ui_map_icon_phigh</Choice>
		<Choice>ui_map_icon_ammo_box</Choice>
		<Choice>ui_map_icon_raid</Choice>
		<Choice>ui_map_icon_convoy</Choice>
		<Choice>ui_map_icon_end_point</Choice>
		<Choice>ui_map_icon_shotgun</Choice>
		<Choice>ui_map_icon_courier</Choice>
		<Choice>ui_map_icon_house_arrest</Choice>
		<Choice>ui_map_icon_demo</Choice>
		<Choice>ui_map_icon_express</Choice>
		<Choice>ui_map_icon_player</Choice>
		<Choice>ui_map_icon_objective</Choice>
		<Choice>ui_map_icon_tank</Choice>
		<Choice>ui_map_icon_carbomb</Choice>
		</Element>
	<Element>
		<Name>Animation</Name>
		<Type>Selection</Type>
		<Description>The name of the animation to play on the tool tip if desired.  (Should be set by MattG or JasonM until a reference table is created).</Description>
		<Required>false</Required>
		<Choice>icon_to_minimap</Choice>
		</Element>
	<Element>
		<Name>Screen_Loc</Name>
		<Type>Selection</Type>
		<Display_Name>Screen Location</Display_Name>
		<Description>The location on the screen the tool tip should be displayed</Description>
		<Choice>North East</Choice>
		<Choice>North West</Choice>
		<Choice>South East</Choice>
		<Choice>South West</Choice>
		<Choice>Center</Choice>
		</Element>
	<Element>
		<Name>Arrow_Loc</Name>
		<Type>Selection</Type>
		<Display_Name>Arrow Location</Display_Name>
		<Description>The direction the arrow on the tool tip should point.</Description>
		<Default>None</Default>
		<Choice>North East</Choice>
		<Choice>Noth West</Choice>
		<Choice>South East</Choice>
		<Choice>South West</Choice>
		<Choice>None</Choice>
		</Element>
	<Element>
		<Name>Play_Times</Name>
		<Type>Int</Type>
		<Display_Name>Number of times to play</Display_Name>
		<Description>The number of times this tool tip should be displayed before it does not get displayed anymore.  (If set to 0, the tool tip will always play).</Description>
		<Default>1</Default>
		</Element>
	<Element>
		<Name>Duration</Name>
		<Type>Int</Type>
		<Description>The duration the tool tip should be displayed (ms).</Description>
