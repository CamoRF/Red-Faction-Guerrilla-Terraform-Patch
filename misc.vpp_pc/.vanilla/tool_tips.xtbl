	<Element>
		<Name>Save_to_Profile</Name>
		<Type>Selection</Type>
		<Display_Name>Save to profile?</Display_Name>
		<Description>If true, we'll save the status of this tooltip to the profile. If false, we'll save it in saved games.</Description>
		<Default>False</Default>
		<Choice>True</Choice>
		<Choice>False</Choice>
		</Element>
	<Element>
		<Name>Always_Show</Name>
		<Type>Selection</Type>
		<Display_Name>Always Show</Display_Name>
