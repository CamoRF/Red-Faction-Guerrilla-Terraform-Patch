	<TableDescription>
		<Name>Character</Name>
		<Type>TableDescription</Type>
		<Description>A game character.</Description>
		<MaxCount>150</MaxCount>
		<Element>
			<Name>Name</Name>
			<Type>String</Type>
			<Description>Unique identifying name, used internally by the game.</Description>
			<Unique>true</Unique>
			<Default>NoName</Default>
			<MaxLength>32</MaxLength>
		</Element>
		<Element>
			<Name>origin_district</Name>
			<Type>Selection</Type>
			<Display_Name>Origin District</Display_Name>
			<Description>District this type of NPC should have been created from</Description>
			<Default>Special</Default>
			<Choice>Oasis</Choice>
			<Choice>Eos</Choice>
			<Choice>Parker</Choice>
			<Choice>Dust</Choice>
			<Choice>Special</Choice>
			<Choice>Common</Choice>
		</Element>
		<Element>
			<Name>Model</Name>
			<Type>Filename</Type>
			<Description>Character model filename, without extension.</Description>
			<Extension>cmeshx</Extension>
			<StartingPath>data</StartingPath>
			<ShowPreload>True</ShowPreload>
		</Element>
		<Element>
			<Name>Rig</Name>
			<Type>Filename</Type>
			<Description>The rig file to use for the character.</Description>
			<Extension>rigx</Extension>
			<StartingPath>data</StartingPath>
			<ShowPreload>True</ShowPreload>
		</Element>
		<Element>
			<Name>Height</Name>
			<Type>Float</Type>
			<Display_Name>Height Scale</Display_Name>
			<Description>how tall the character is as a ratio of model height</Description>
			<Default>1.0</Default>
		</Element>
		<Element>
			<Name>Variation</Name>
			<Type>Float</Type>
			<Display_Name>Height Variation</Display_Name>
			<Description>max deviation from height scale for each char instance</Description>
			<Default>0.0</Default>
		</Element>
		<Element>
			<Name>max_hit_points</Name>
			<Type>Int</Type>
			<Display_Name>Max Hit Points</Display_Name>
			<Default>300</Default>
		</Element>
		<Element>
			<Name>knock_down_hits</Name>
			<Type>Int</Type>
			<Display_Name>Knock Down Hits</Display_Name>
			<Default>30</Default>
			<MinValue>0</MinValue>
		</Element>
