			<Default>5</Default>
		</Element>
		<Element>
			<Name>AimErrorPct</Name>
			<Type>Int</Type>
			<Display_Name>Aim Error Percent</Display_Name>
			<Description>Amount of aim drift error, relative to a normal character (higher is worse aim, less is better)</Description>
			<Default>100</Default>
			<MinValue>50</MinValue>
			<MaxValue>150</MaxValue>
			<UseSlider>True</UseSlider>
		</Element>
